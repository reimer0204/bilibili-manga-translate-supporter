{
	"name": "bilibili漫画 翻訳表示ツール",
	"version": "1.0.2",
	"manifest_version": 2,
	"description": "bilibili漫画に翻訳テキストを表示・編集できるようになる拡張機能です",
	"content_scripts": [
		{
			"matches": ["https://manga.bilibili.com/mc*"],
			"js": [
				"js/chunk-vendors.js",
				"js/app.js"
			]
		}
	],
	"homepage_url": "https://github.com/reimer0204/bilibili-manga-translate-supporter",

	"icons": {
		"16":  "icon/16.png",
		"48":  "icon/48.png",
		"128": "icon/128.png"
	},

	"permissions": [
		"https://manga.bilibili.com/mc*"
	]
}